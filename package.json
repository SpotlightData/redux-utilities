{
  "name": "redux-utilities",
  "version": "1.0.0",
  "description": "Utility set for redux",
  "main": "dist/index.js",
  "scripts": {
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/redux-utilities.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/redux-utilities.min.js",
    "build": "npm run build:umd && npm run build:umd:min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpotlightData/redux-utilities.git"
  },
  "author": "",
